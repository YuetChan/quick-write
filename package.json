{
  "name": "quick-write",
  "displayName": "quick-write",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.52.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "keybindings": [
      {
        "command": "quick-write.openNote",
        "key": "ctrl+f1",
        "mac": "cmd+f1"
      }
    ],
    "commands": [
      {
