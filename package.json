    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5",
    "vscode-test": "^1.6.1"
  },
  "dependencies": {
    "uuidv4": "^6.2.13"
  },
  "repository": {
    "name": "quick-write",
