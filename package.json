{
  "name": "code-meta-ext",
  "displayName": "code-meta-ext",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.52.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "keybindings": [
      {
        "command": "code-meta-ext.openNote",
        "key": "ctrl+f1",
        "mac": "cmd+f1"
      }
    ],
    "commands": [
      {
        "command": "code-meta-ext.openNote",
        "title": "open file",
        "category": "code-meta"
      },
      {
        "command": "code-meta-ext.clearCache",
        "title": "clear cache",
        "category": "code-meta"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.77.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "uuidv4": "^6.2.13"
  }
}
